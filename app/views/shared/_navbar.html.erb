<div class="navbar navbar-expand-sm navbar-light navbar-lewagon", id="navbar">
  <a class="navbar-brand">
    <div class="logo" style="background-image: url(<%= image_path "logo.png" %>)">
   </div>
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation" return="false">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <div class="containery">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <%= link_to "Home", root_path, class: "nav-link" %>
        </li>
        <li class="nav-item">
          <% if user_signed_in? %>
            <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "nav-link" %>
          <% else %>
            <%= link_to "Sign In", new_user_session_path, class: "nav-link" %>
          <% end %>
        </li>
        <li class="nav-item active">
          <% if user_signed_in? %>
            <%= link_to "My Bookings", my_bookings_path, class: "nav-link" %>
          <% end %>
        </li>
        <li class="nav-item active">
          <% if user_signed_in? %>
            <%= link_to "Profile", show_user_dashboard_path, class: "nav-link" %>
          <% end %>
        </li>
        <li class="nav-item dropdown">
          <% if user_signed_in? %>
          <img class="avatar dropdown-toggle" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" src="https://kitt.lewagon.com/placeholder/users/ssaunier" />
          <% end %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <a class="dropdown-item" href="#">Action</a>
            <a class="dropdown-item" href="#">Another action</a>
          </div>
